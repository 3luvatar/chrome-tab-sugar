{
  "name": "Tab Sugar",
  "version": "0.9.22",
  "description": "Organize and manage your tabs from a single page!",
  "icons": {
    "16": "ico/16.png",
    "48": "ico/48.png",
    "128": "ico/128.png"
  },
  "browser_action": {
    "default_icon": "ico/browser_action_loading.png",
    "default_title": "Loading Tab Sugar..."
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*",
        "ftp://*/*"
      ],
      "js": [
        "js/lib/jquery.1.4.2.min.js",
        "js/lib/jquery.hotkeys-0.7.9.min.js",
        "js/activate_hotkey.js"
      ],
      "run_at": "document_start"
    }
  ],
  "background_page": "background.html",
  "options_page": "options.html",
  "minimum_chrome_version": "6",
  "permissions": [
    "tabs",
    "unlimited_storage",
    "chrome://favicon/",
    "http://tabsugar.com/",
    "bookmarks",
    "idle",
    "geolocation",
    "contextMenus"
  ]
}
